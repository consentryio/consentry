    "directory": "consentry-next"
  },
  "bugs": {
    "url": "https://github.com/consentryio/consentry/issues"
  },
  "author": {
    "name": "Mustafa ONAL",
    "email": "mustafasaitonal@outlook.com",
    "url": "https://github.com/neddl"
  },
